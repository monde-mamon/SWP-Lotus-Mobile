{
  "name": "swp-lotus-tracker",
  "main": "node_modules/expo/AppEntry.js",
  "version": "0.0.8",
  "scripts": {
    "build-ios": "bunx expo run:ios --device",
    "build-android": "bunx expo run:android --device",
    "local-ios-prod": "rm -rf ./ios && bunx expo run:ios --configuration Release --device",
    "local-android-prod": "rm -rf ./android && bunx expo run:android --variant release --device",
    "reset": "rm -rf node_modules ./android bun.lockb && bun install",
    "android": "TAMAGUI_TARGET=native bun run expo start --android",
    "ios": "TAMAGUI_TARGET=native bun run expo start --ios",
    "tsc": "tsc",
    "lint": "eslint . --ext .ts,.tsx,.js,jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,jsx --fix",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "dev-web": "TAMAGUI_TARGET=web bun run expo start --web --clear --port=4201",
    "dev": "TAMAGUI_TARGET=native bun run expo start -c",
    "prepare": "husky install",
    "build-ios-develop": "eas build --platform ios --profile develop --non-interactive",
    "build-android-develop": "eas build --platform android --profile develop --non-interactive",
    "build-android-preview": "eas build --platform android --profile preview --non-interactive",
    "build-all-develop": "eas build --platform all --profile develop --non-interactive",
    "build-all-preview": "eas build --platform all --profile preview --non-interactive",
    "reactotron": "adb reverse tcp:9090 tcp:9090"
  },
  "jest": {
    "preset": "jest-expo"
  },
